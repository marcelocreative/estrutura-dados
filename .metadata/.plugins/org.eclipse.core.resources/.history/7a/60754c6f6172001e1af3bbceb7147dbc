package com.mg.estruturadados.vetor.testes;

import com.mg.estruturadados.vetor.Lista;

public class Teste010 {

	public static void main(String[] args) {
		
		Lista<Contato> contatos= new Lista<Contato>(3);
		
		Contato contato1= new Contato("Contato1", "1234-5678", "contato1@gmail.com");
		Contato contato2= new Contato("Contato2", "2345-6789", "contato2@gmail.com");
		Contato contato3= new Contato("Contato3", "3456-7890", "contato3@gmail.com");
		
		contatos.adicionar(contato1);
		contatos.adicionar(contato2);
		contatos.adicionar(contato3);
		
		System.out.println(contatos);
	}

}
