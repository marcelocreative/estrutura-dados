package com.mg.estruturadados.vetor.testes;

import com.mg.estruturadados.vetor.Vetor;

public class Teste008 {

	public static void main(String[] args) {
		
		Vetor vetor= new Vetor(3);
		
		vetor.adicionar("A");
		vetor.adicionar("B");
		vetor.adicionar("G");
		vetor.adicionar("C");
		vetor.adicionar("D");
		
		System.out.println(vetor);
		
		vetor.remover(4);
		
		System.out.println(vetor);

	}

}
