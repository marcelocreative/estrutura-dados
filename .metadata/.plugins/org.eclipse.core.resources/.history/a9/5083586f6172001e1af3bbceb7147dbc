package com.mg.estruturadados.vetor.desafios;

import com.mg.estruturadados.vetor.Lista;

public class Desafio002 {

	public static void main(String[] args) {
	
		Lista<String> lista= new Lista<>(10);

		lista.adicionar("A");
		lista.adicionar("B");
		lista.adicionar("C");
		lista.adicionar("C");
		lista.adicionar("D");
		lista.adicionar("E");
		lista.adicionar("F");
		lista.adicionar("F");
		lista.adicionar("F");
		lista.adicionar("F");
		
		String elemento= "H";
		
		System.out.print("Último índice do elemento "+elemento+": ");
		System.out.println(lista.ultimoIndice(elemento));
	}

}
