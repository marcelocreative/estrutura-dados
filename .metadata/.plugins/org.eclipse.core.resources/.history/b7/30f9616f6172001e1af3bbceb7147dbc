package com.mg.estruturadados.pilha.testes;

import com.mg.estruturadados.pilha.Pilha;

public class Teste005 {

	public static void main(String[] args) {
		
		Pilha<Integer> pilha= new Pilha<Integer>();
		
		pilha.empilhar(1);
		pilha.empilhar(2);
		pilha.empilhar(3);
		
		System.out.println(pilha);
		
		System.out.println("Desempilhando o elemento "+pilha.desempilhar());
		
		System.out.println(pilha);
		System.out.println("Tamanho da pilha: "+pilha.getQuantidadeElementosVetor());
	}
	
}
